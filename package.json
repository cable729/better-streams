{
  "name": "better-streams",
  "version": "1.0.0",
  "description": "Better stream-watching experience for League of Legends",
  "scripts": {
    "start": "cross-env PORT=80 nodemon dist/index.js",
    "heroku-postbuild": "babel -D src/server -d dist/",
    "watch-server": "nodemon --watch src/server --exec babel-node src/server/index.js",
    "test": "mocha src/**/*.tests.js --compilers js:babel-register",
    "dev": "node node_modules\\webpack-dev-server\\bin\\webpack-dev-server.js --hot --inline --port=3000",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/better-streams.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "stream",
    "league",
    "legends",
    "lol",
    "twitch",
    "twitchtv"
  ],
  "author": "Caleb Jares <cable729@gmail.com> (http://github.com/cable729)",
  "license": "MIT",
  "engines": {
    "node": "6.2.0"
  },
  "bugs": {
    "url": "https://github.com/cable729/better-streams/issues"
  },
  "homepage": "https://github.com/cable729/better-streams",
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^2.10.2",
    "eslint-plugin-react": "^5.1.1",
    "mocha": "^2.5.3",
    "nodemon": "^1.9.2",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.0.20",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.0.1",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.9.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "cross-env": "^1.0.8",
    "ejs": "^2.4.2",
    "express": "^4.13.4",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "request-promise": "^3.0.0"
  }
}
